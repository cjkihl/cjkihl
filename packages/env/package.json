{
  "name": "@cjkihl/env",
  "version": "0.0.5",
  "description": "Type-safe environment variable validation and management with Zod schemas",
  "keywords": [
    "environment",
    "env",
    "validation",
    "zod",
    "typescript",
    "type-safe",
    "config",
    "variables",
    "schema",
    "bun",
    "node"
  ],
  "homepage": "https://github.com/cjkihl/cjkihl#readme",
  "bugs": {
    "url": "https://github.com/cjkihl/cjkihl/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cjkihl/cjkihl.git"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/cjkihl"
  },
  "license": "MIT",
  "author": "@cjkihl",
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/output/index.pub.js",
      "types": "./dist/types/index.pub.d.ts"
    },
    "./create-env-file": {
      "default": "./dist/output/create-env-file.pub.js",
      "types": "./dist/types/create-env-file.pub.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && bun ../create-exports/create-exports.bin.ts",
    "test": "bun test",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "remeda": "2.32.0",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@cjkihl/tsconfig": "workspace:*",
    "@types/node": "^24.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
