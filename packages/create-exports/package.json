{
  "name": "@cjkihl/create-exports",
  "version": "0.1.12",
  "description": "Tool to generate package exports configuration for TypeScript packages",
  "keywords": [
    "typescript",
    "exports",
    "package",
    "configuration",
    "build-tool"
  ],
  "homepage": "https://github.com/cjkihl/cjkihl#readme",
  "bugs": {
    "url": "https://github.com/cjkihl/cjkihl/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cjkihl/cjkihl.git"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/cjkihl"
  },
  "license": "MIT",
  "author": "@cjkihl",
  "type": "module",
  "exports": {
    "./create-exports": {
      "types": "./.dist/types/create-exports.pub.d.ts",
      "default": "./.dist/output/create-exports.pub.js"
    }
  },
  "bin": {
    "create-exports": "./.dist/output/create-exports.bin.js"
  },
  "files": [
    ".dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && bun ../create-exports/create-exports.bin.ts",
    "publish-npm": "bun ../publish/safe-publish.bin.ts",
    "test": "bun test"
  },
  "dependencies": {
    "@cjkihl/find-root": "workspace:*",
    "fast-glob": "^3.3.3",
    "type-fest": "^4.41.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
