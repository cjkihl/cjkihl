{
	"name": "@cjkihl/create-exports",
	"version": "0.0.1",
	"description": "Tool to generate package exports configuration for TypeScript packages",
	"author": "@cjkihl",
	"license": "MIT",
	"type": "module",
	"exports": {
		".": {
			"types": "./.dist/types/index.d.ts",
			"default": "./.dist/output/index.js"
		}
	},
	"bin": {
		"create-exports": "./.dist/output/index.bin.js"
	},
	"files": [
		".dist",
		"README.md"
	],
	"scripts": {
		"build": "tsc && chmod +x .dist/output/index.bin.js"
	},
	"keywords": [
		"typescript",
		"exports",
		"package",
		"configuration",
		"build-tool"
	],
	"dependencies": {
		"@cjkihl/find-root": "workspace:*",
		"fast-glob": "^3.3.3",
		"type-fest": "^4.41.0"
	},
	"devDependencies": {
		"@cjkihl/tsconfig": "workspace:*",
		"bun-types": "latest",
		"typescript": "^5.0.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
