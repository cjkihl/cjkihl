{
  "name": "@cjkihl/cookies",
  "version": "1.0.5",
  "description": "A minimal, type-safe cookies library for Bun/Node.js with Standard Schema validation support",
  "keywords": [
    "cookies",
    "cookie",
    "typescript",
    "standard-schema",
    "validation",
    "zod",
    "valibot",
    "arktype",
    "bun",
    "node",
    "browser",
    "server"
  ],
  "homepage": "https://github.com/cjkihl/cjkihl#readme",
  "bugs": {
    "url": "https://github.com/cjkihl/cjkihl/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cjkihl/cjkihl.git"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/cjkihl"
  },
  "license": "MIT",
  "author": "@cjkihl",
  "type": "module",
  "exports": {
    "./client": {
      "default": "./dist/output/client.pub.js",
      "types": "./dist/types/client.pub.d.ts"
    },
    "./server": {
      "default": "./dist/output/server.pub.js",
      "types": "./dist/types/server.pub.d.ts"
    },
    "./types": {
      "default": "./dist/output/types.pub.js",
      "types": "./dist/types/types.pub.d.ts"
    },
    "./utils": {
      "default": "./dist/output/utils.pub.js",
      "types": "./dist/types/utils.pub.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && bun ../create-exports/create-exports.bin.ts",
    "test": "bun test",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@standard-schema/spec": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "zod": "^4.1.12"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
